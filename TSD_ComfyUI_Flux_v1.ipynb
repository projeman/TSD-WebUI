    "    Version = '1.0'\n",
    "    print('✅ TSD kurulum tamamlandı!')\n",
    "\n",
    "print(f'🚀 TSD Versiyon: {Version}')\n",
    "\n",
    "# Türkçe Seçenekler\n",
    "HizliMod = True #@param {type:\"boolean\"}\n",
    "UI_Versiyonu = \"ComfyUI\"\n",
