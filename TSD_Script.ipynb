    "# @title # 📦 Model İndirme Sistemi\n",
    "# Türkçe model indirme ve kurulum fonksiyonları\n",
    "\n",
    "def model_indir(baglanti):\n",
    "    \"\"\"Model indirme ana fonksiyonu\"\"\"\n",
    "    indirme_kodu = 'aria2c --console-log-level=error -c -x 16 -s 16 -k 1M'\n",
    "    baglanti = baglanti.replace('&', '\\&')\n",
    "    deger_baglanti, deger_isim = baglanti_ayir(baglanti)\n",
    "    deger_baglanti = deger_baglanti.split(\"?\")[0]\n",
    "    deger_baglanti = baglanti_kontrol(deger_baglanti)\n",
    "    \n",
    "    if '#' not in baglanti:\n",
    "        if deger_isim != None:\n",
    "            if 'civit' not in baglanti:\n",
    "                !{indirme_kodu} {deger_baglanti} -d {checkpoint_yolu} -o {deger_isim}\n",
    "            else:\n",
    "                !wget {deger_baglanti}{token_al(deger_baglanti)} -O {checkpoint_yolu}/{deger_isim}\n",
    "        else:\n",
    "            !wget {deger_baglanti}{token_al(deger_baglanti)} -P {checkpoint_yolu} --content-disposition\n",
    "\n",
    "def baglanti_ayir(baglanti):\n",
    "    \"\"\"Bağlantı ve dosya adını ayır\"\"\"\n",
    "    ayirici = \"@=\"\n",
    "    if ayirici in baglanti:\n",
    "        parcalar = baglanti.split(ayirici)\n",
    "        if '.ckpt' not in parcalar[1] and '.gguf' not in parcalar[1] and '.safetensors' not in parcalar[1]:\n",
    "            parcalar[1] = f'{parcalar[1]}.safetensors'\n",
    "        return parcalar[0], parcalar[1]\n",
    "    else:\n",
    "        return baglanti, None\n",
    "\n",
    "def controlnet_indir(baglanti):\n",
    "    \"\"\"ControlNet model indirme\"\"\"\n",
    "    indirme_kodu = 'aria2c --console-log-level=error -c -x 16 -s 16 -k 1M'\n",
    "    deger_isim = baglanti.split('/')[-1].split('?')[0]\n",
    "    !{indirme_kodu} {baglanti} -d {controlnet_yolu} -o {deger_isim}\n",
    "\n",
    "def kutuphane_modeli_indir(model, model_listesi):\n",
    "    \"\"\"Kütüphaneden model indirme\"\"\"\n",
    "    indirme_kodu = 'aria2c --console-log-level=error -c -x 16 -s 16 -k 1M'\n",
    "    if 'https:' in model:\n",
    "        model_indir(model)\n",
    "    else:\n",
    "        if not any(ext in model for ext in ['.ckpt', '.gguf', '.safetensors']):\n",
    "            model += '.safetensors'\n",
    "        if model in model_listesi:\n",
    "            !{indirme_kodu} {model_listesi[model]} -d {checkpoint_yolu} -o {model}\n",
    "\n",
    "def txt_indir(satirlar):\n",
    "    \"\"\"Txt dosyasından model indirme\"\"\"\n",
    "    for satir in satirlar:\n",
    "        model_indir(satir)\n",
    "\n",
    "def controlnet_txt_indir(txt_adi):\n",
    "    \"\"\"ControlNet txt dosyasından indirme\"\"\"\n",
    "    if '/' in txt_adi:\n",
    "        dosya_yolu = txt_adi\n",
    "    else:\n",
    "        dosya_yolu = f'{controlnet_yolu}/{txt_adi}'\n",
    "    \n",
    "    with open(dosya_yolu, 'r') as dosya:\n",
    "        satirlar = dosya.readlines()\n",
    "    satirlar = [satir.strip() for satir in satirlar]\n",
    "    \n",
    "    for satir in satirlar:\n",
    "        if '#' not in satir:\n",
    "            controlnet_indir(satir)\n",
    "\n",
    "print('📦 Model indirme sistemi hazır!')"
   ]
