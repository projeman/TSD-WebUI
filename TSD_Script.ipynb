    "# @title # 🇹🇷 TSD-WebUI Ana Script\n",
    "# Turkish Stable Diffusion WebUI - Ana kurulum scripti\n",
    "# Çeviren: Turkish AI Community | Orijinal: SDVN\n",
    "\n",
    "import os,subprocess,sys,fileinput,time,requests,json,subprocess,gc,threading,time,re,socket,urllib.request,ipywidgets as widgets\n",
    "from datetime import datetime\n",
    "from subprocess import getoutput\n",
    "from IPython.utils import capture\n",
    "from PIL import Image\n",
    "from IPython.display import clear_output,display, HTML, Image\n",
    "from io import BytesIO\n",
    "import zipfile, random, string\n",
    "!pip install python-multipart\n",
    "\n",
    "# 🇹🇷 Türkçe Konfigürasyon Değerleri\n",
    "varsayilan_degerler = {\n",
    "    'SürücüSenkron': True,\n",
    "    'Ayar_Onceden_Secili': \"Yok\",\n",
    "    'Kutuhane': True,\n",
    "    'HizliMod': True,\n",
    "    'ana_klasor': '/content',\n",
    "    'Kullanici_klasoru': '/content/drive/MyDrive/TSD-Data',\n",
    "    'API_klasoru': '/content/TSD-WebUI',\n",
    "    'TSDKlasoru': '/content/TSD-WebUI',\n",
    "    'Versiyon': 'ComfyUI',\n",
    "    'SecenekModu': 'temel',\n",
    "    'Controlnet': 'yok',\n",
    "    'SDmodel': '',\n",
    "    'SD15model': '',\n",
    "    'SDXLmodel': '',\n",
    "    'Fluxmodel': '',\n",
    "    'TSDmodel': '',\n",
    "    'InpaintMd': '',\n",
    "    'SurucuKutuphane': True,\n",
    "    'KomutSatiri': '',\n",
    "    'frontend_klasoru': '/usr/local/lib/python3.12/dist-packages/comfyui_frontend_package/static',\n",
    "}\n",
    "\n",
    "# Global değişkenleri kontrol et ve ata\n",
    "if 'UI_Versiyonu' in globals():\n",
    "    Versiyon = UI_Versiyonu\n",
    "    \n",
    "for anahtar, deger in varsayilan_degerler.items():\n",
    "    if anahtar not in globals():\n",
    "        globals()[anahtar] = deger\n",
    "    elif type(globals()[anahtar]) == str:\n",
    "        globals()[anahtar] = globals()[anahtar].split(' ')[-1]\n",
    "\n",
    "# 🛠️ Yardımcı Fonksiyonlar\n",
    "def dosya_metni_degistir(dosya_yolu, eski_metin, yeni_metin):\n",
    "    \"\"\"Dosyadaki metni değiştir\"\"\"\n",
    "    with open(dosya_yolu, 'r', encoding='utf-8') as dosya:\n",
    "        dosya_icerigi = dosya.read()\n",
    "    guncellenmis_icerik = dosya_icerigi.replace(eski_metin, yeni_metin)\n",
    "    with open(dosya_yolu, 'w', encoding='utf-8') as dosya:\n",
    "        dosya.write(guncellenmis_icerik)\n",
    "\n",
    "def bilgi_goster(mesaj, stil, genislik):\n",
    "    \"\"\"Bilgi butonu göster\"\"\"\n",
    "    bilgi_butonu = widgets.Button(\n",
    "        description=mesaj, \n",
    "        disabled=True, \n",
    "        button_style=stil, \n",
    "        layout=widgets.Layout(min_width=genislik)\n",
    "    )\n",
    "    display(bilgi_butonu)\n",
    "\n",
    "def aria_indir(baglanti, yol, isim):\n",
    "    \"\"\"Aria2c ile dosya indir\"\"\"\n",
    "    !aria2c --console-log-level=error -c -x 16 -s 16 -k 1M {baglanti} -d {yol} -o {isim}\n",
    "\n",
    "def klasor_olustur(yol):\n",
    "    \"\"\"Kullanıcı klasöründe alt klasör oluştur\"\"\"\n",
    "    yol = f'{Kullanici_klasoru}/{yol}'\n",
    "    os.makedirs(yol, exist_ok=True)\n",
    "\n",
    "def varsayilan_dosya_kontrol(kaynak_klasor, hedef_klasor, uzerine_yaz=False):\n",
    "    \"\"\"Varsayılan dosyaları kopyala\"\"\"\n",
    "    for oge in os.listdir(kaynak_klasor):\n",
    "        dosya_yolu = os.path.join(kaynak_klasor, oge)\n",
    "        if os.path.isfile(dosya_yolu):\n",
    "            cp_yolu = dosya_yolu.replace(TSDKlasoru, hedef_klasor)\n",
    "            !cp {'-f' if uzerine_yaz else '-n'} {dosya_yolu} {cp_yolu}\n",
    "        elif os.path.isdir(dosya_yolu):\n",
    "            varsayilan_dosya_kontrol(dosya_yolu, hedef_klasor, uzerine_yaz)\n",
    "\n",
    "def civit_indirme_baglantisi(baglanti):\n",
    "    \"\"\"CivitAI bağlantısını API linkine çevir\"\"\"\n",
    "    !wget {baglanti} -q -O model.html\n",
    "    try:\n",
    "        with open('model.html', 'r', encoding='utf-8') as dosya:\n",
    "            html_icerigi = dosya.read()\n",
    "        desen = r'\"modelVersionId\":(\\d+),'\n",
    "        model_id = re.findall(desen, html_icerigi)\n",
    "        if model_id:\n",
    "            api_baglanti = f'https://civitai.com/api/download/models/{model_id[0]}'\n",
    "            print(f'Model indirme ID bağlantısı: {api_baglanti}')\n",
    "            return api_baglanti\n",
    "        else:\n",
    "            return \"Uygun içerik bulunamadı.\"\n",
    "    except requests.RequestException as e:\n",
    "        return f\"Sayfa yükleme hatası: {e}\"\n",
    "\n",
    "def baglanti_kontrol(baglanti):\n",
    "    \"\"\"Bağlantı formatını kontrol et ve düzelt\"\"\"\n",
    "    if 'huggingface.co' in baglanti:\n",
    "        if 'blob' in baglanti:\n",
    "            baglanti = baglanti.replace('blob', 'resolve')\n",
    "            return baglanti\n",
    "        else:\n",
    "            return baglanti\n",
    "    if 'civitai.com' in baglanti:\n",
    "        if 'civitai.com/models' in baglanti:\n",
    "            return civit_indirme_baglantisi(baglanti)\n",
    "        else:\n",
    "            return baglanti\n",
    "\n",
    "def token_al(baglanti):\n",
    "    \"\"\"API token'ı al\"\"\"\n",
    "    if \"civitai\" in baglanti:\n",
    "        token = f'?token=8c7337ac0c39fe4133ae19a3d65b806f'  # 🇹🇷 Kendi token'ınızı ekleyin\n",
    "    else:\n",
    "        token = \"\"\n",
    "    return token\n",
    "\n",
    "# 🌐 Tunnel Fonksiyonları\n",
    "def cloudflare_ayar_kontrol():\n",
    "    \"\"\"Cloudflare ayarlarını kontrol et\"\"\"\n",
    "    domain_ayari = f\"{Kullanici_klasoru}/Setting/Domain_sever.txt\"\n",
    "    with open(domain_ayari, \"r\", encoding=\"utf-8\") as dosya:\n",
    "        satirlar = dosya.readlines()\n",
    "    cikti = [None, None]\n",
    "    for satir in satirlar:\n",
    "        if \"cloudflare\" in satir and \"#\" not in satir:\n",
    "            flare_token = satir.split(\"-\")[-1].strip()\n",
    "            flare_domain = satir.split(\"-\")[1].strip()\n",
    "            cikti = [flare_token, flare_domain]\n",
    "    return cikti\n",
    "\n",
    "def cloudflare_thread(port):\n",
    "    \"\"\"Cloudflare tunnel başlat\"\"\"\n",
    "    import socket\n",
    "    while True:\n",
    "        time.sleep(0.5)\n",
    "        sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "        sonuc = sock.connect_ex(('127.0.0.1', port))\n",
    "        if sonuc == 0:\n",
    "            break\n",
    "        sock.close()\n",
    "    \n",
    "    flare_token, flare_domain = cloudflare_ayar_kontrol()\n",
    "    if flare_token != None:\n",
    "        subprocess.Popen([\"cloudflared\", \"tunnel\", \"run\", \"--token\", flare_token])\n",
    "        print(f\"\\033[92m{'🔗 Özel Cloudflare bağlantısı:'}\\ 033[0m\", f\"https://{flare_domain}\")\n",
    "    \n",
    "    p = subprocess.Popen([\"cloudflared\", \"tunnel\", \"--url\", \"http://127.0.0.1:{}\".format(port)], \n",
    "                        stdout=subprocess.PIPE, stderr=subprocess.PIPE)\n",
    "    for line in p.stderr:\n",
    "        l = line.decode()\n",
    "        if \"trycloudflare.com \" in l:\n",
    "            print(f\"\\033[92m{'🔗 Online bağlantı:'}\\ 033[0m\", l[l.find(\"http\"):], end='')\n",
    "            html = \"<div><code style='color:yellow'>🇹🇷 TSD Hızlı Mod aktif! Bu modda daha hızlı başlatma için daha az node yüklenir. ControlNet/IPAdapter otomatik yüklenmez. Tam özellik için Hızlı Mod'u kapatın ve tekrar çalıştırın.</code></div>\"\n",
    "            if HizliMod:\n",
    "                display(HTML(html))\n",
    "\n",
    "def sunucu_flare_baslat(port, api=None):\n",
    "    \"\"\"Tunnel sunucularını başlat\"\"\"\n",
    "    if api != None:\n",
    "        # Tunnelto için thread başlat (gerekirse)\n",
    "        pass\n",
    "    threading.Thread(target=cloudflare_thread, daemon=True, args=(port,)).start()\n",
    "\n",
    "print('🇹🇷 TSD-WebUI Ana Script Yüklendi!')\n",
    "print('🚀 Kurulum hazır, bir sonraki hücreye geçin!')"
   ]
